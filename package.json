{
  "name": "Code-Push",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "ajv": "^7.2.1",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "envalid": "^7.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "oauth2-api-azure": "^0.1.7",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2"
  },
  "scripts": {
    "start": "yarn && node server.js",
    "predev": "yarn && yarn docker-up",
    "dev": "nodemon src/server.js",
    "docker-up": "docker-compose -f docker-compose-dev.yml up -d",
    "postdocker-up": "sleep 5 && yarn run-migration",
    "run-migration": "yarn sequelize db:migrate",
    "docker-down": "docker-compose -f docker-compose-dev.yml down",
    "dev:debug": "nodemon --inspect src/server.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src/** --ext .js --ignore-pattern src/key/*.pem"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sucrase": "^3.10.1"
  }
}
